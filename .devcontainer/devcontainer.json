{
    "name": "DS-Lab",
    "dockerComposeFile": "docker-compose.yml",
    "service": "jupyter",
    "workspaceFolder": "/home/work",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance",
          "ms-toolsai.jupyter"
        ],
        "settings": {
          "editor.tabSize": 2,
          "python.defaultInterpreterPath": "/opt/conda/bin/python"
        }
      }
    }
  }